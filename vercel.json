{

  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next",
      "buildCommand": "npm run build",
      "installCommand": "npm install",
      "outputDirectory": "frontend/.next",
      "framework": "nextjs"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ]
}
